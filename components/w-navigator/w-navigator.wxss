.navigator-container {
  background-color: white;
}
.search-container {
  display: flex;
  background-color: white;
  width: 100%;
}
.hide-search {
  display: none;
}
.nav-img {
  width: 100%;
  margin-left: 20rpx;
}
.hide-img {
  display: none;
}
.navigator-search {
  width: 100%;
  height: 100%;
  background-color: rgba(128, 128, 128, 0.6);
  margin-left: 20rpx;
  display:flex;
}
.navigator-search .search-title {
  height: 100%;
  flex: 1;
}
.navigator-search .swiper_item {
  height: 100%;
  line-height: 58rpx;
}
.navigator-search-button {
  width: 100rpx;
  height: 100%;
  border-radius: 0 40rpx 40rpx 0;
  color: white;
  display: flex;
}
.navigator-search-button text {
  width: 100rpx;
  height: 100%;
  background-color: #ee423e;
  border-radius: 40rpx;
  text-align: center;
  line-height: 58rpx;
}
.clear-button {
  display: none;
}